{
  "$schema" : "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
  "id": "http://standardhealthrecord.org/schemas/shr/identification/valuesets.schema.json",
  "description": "Identification ValueSets defined by SHR",
  "definitions": {
    "HumanNamePrefix": {
      "$ref": "http://standardhealthrecord.org/schemas/shr/types.schema.json#/definitions/concept",
      "valueSet": "http://standardhealthrecord.org/identification/vs/HumanNamePrefix",
      "enum": [
        {"code": "mr", "display": "Mr."},
        {"code": "mrs", "display": "Mrs."},
        {"code": "ms", "display": "Ms."},
        {"code": "dr", "display": "Dr."}
      ]
    },
    "HumanNameSuffix": {
      "$ref": "http://standardhealthrecord.org/schemas/shr/types.schema.json#/definitions/concept",
      "valueSet": "http://standardhealthrecord.org/identification/vs/HumanNameSuffix",
      "enum": [
        {"code": "jr", "display": "Jr."},
        {"code": "sr", "display": "Sr."},
        {"code": "md", "display": "M.D."},
        {"code": "phd", "display": "Ph.D"}
      ]
    },
    "HumanNameUse": {
      "$ref": "http://standardhealthrecord.org/schemas/shr/types.schema.json#/definitions/concept",
      "valueSet": "http://standardhealthrecord.org/identification/vs/HumanNameUse",
      "enum": [
        {"code": "preferred", "display": "Preferred"},
        {"code": "legal", "display": "Legal"},
        {"code": "alias", "display": "Alias or Pseudonym"},
        {"code": "premarriage", "display": "Premarriage Name"},
        {"code": "variant", "display": "Spelling Variant"},
        {"code": "temporary", "display": "Temporary"}
      ]
    }
  }
}