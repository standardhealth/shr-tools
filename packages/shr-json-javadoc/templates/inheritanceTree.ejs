<% if (index == 0) { -%>
  <% if (setClass) { -%>
  <li class="last">
    <a href="../<%= ancestor.path %>/<%= ancestor.name %>.html"><%= ancestor.name %></a>
  </li>
  <% } else { -%>
  <li>
  <a href="../<%= ancestor.path %>/<%= ancestor.name %>.html"><%= ancestor.name %></a>
  </li>
  <% } -%>
<% } else { -%>
  <li>
    <ul>
      <%- include('inheritanceTree', { ancestor: ancestor, index: index-1, setClass: true }) -%>
    </ul>
  </li>
<% } -%>