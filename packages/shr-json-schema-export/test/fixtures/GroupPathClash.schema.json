    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://standardhealthrecord.org/schema/cimpl/builtin",
    "title": "CIMPL Builtin Schema",
    "definitions": {
      "Concept": {
        "type": "object",
        "properties": {
          "coding": {
            "type": "array",
            "minItems": 0,
            "items": {
              "$ref": "#/definitions/Coding"
            }
          }
        },
        "required": [
          "coding"
        ]
      },
      "Coding": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "system": {
            "type": "string",
            "format": "uri"
          },
          "display": {
            "type": "string"
          }
        }
      }
    }
  },
